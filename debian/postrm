#!/bin/sh
set -e

#DEBHELPER#

case $1 in
    remove)
        conflicts="
        /etc/skel/.face
        /usr/share/gnome-shell/gnome-shell-theme.gresource
        /usr/share/sddm/themes/breeze/theme.conf.user
        "
        for i in $conflicts
        do
            dpkg-divert --package radxa-desktop-branding --rename --remove "$i"
        done
        ;;
esac

exit 0
